{
    "name": "yab/quazar",
    "description": "An E-Commerce platform for Laravel Apps using Quarx",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.4",
        "laravel/cashier": "^7.0",
        "mpociot/vat-calculator": "^2.2"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "orchestra/testbench": "3.4.*",
        "mikey179/vfsStream": "^1.6",
        "yab/laracogs": "^2.2",
        "yab/quarx": "^2.3",
        "orchestra/database": "^3.4",
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench-browser-kit": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Yab\\Quazar\\": "src/"
        },
        "files": [
          "src/Helpers/StoreHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "fixture/Controllers",
            "fixture/Models",
            "fixture/Services",
            "src/Publishes"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
