{
    "name": "sierratecnologia/commerce",
    "description": "An E-Commerce platform for Laravel Apps using Sitec CMS",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.6",
        "laravel/cashier": "^7.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "orchestra/testbench": "3.6.*",
        "mikey179/vfsStream": "^1.6",
        "sierratecnologia/builder": "dev-master",
        "sierratecnologia/cms": "dev-master",
        "orchestra/database": "^3.6",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench-browser-kit": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Sitec\\Commerce\\": "src/"
        },
        "files": [
          "src/Helpers/commerce_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/Factories",
            "tests/TestCase.php",
            "fixture/Controllers",
            "fixture/Models",
            "fixture/Services",
            "src/Publishes"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Sitec\\Commerce\\SitecCommerceModuleProvider"
            ]
        }
    }
}
