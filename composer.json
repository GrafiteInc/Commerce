{
    "name": "yab/hadron",
    "type": "laravel-library",
    "description": "An E-Commerce platform for Laravel Apps",
    "license": "MIT",
    "require": {
        "illuminate/support": "5.3",
        "laravel/cashier": "^7.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "orchestra/testbench": "~3.0",
        "mikey179/vfsStream": "^1.6",
        "yab/laracogs": "^2.1",
        "yab/quarx": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Quarx\\Modules\\Hadron\\": "src/"
        },
        "files": [
          "src/Helpers/StoreHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "fixture/Controllers",
            "src/Publishes",
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-name": "Hadron"
    }
}
